{
  "name": "company.service.resilientemail",
  "version": "1.0.0",
  "description": "An api to provide resilient email service",
  "main": "app.js",
  "scripts": {
    "builddev": "rmdir /s /q dist && webpack --mode development --config webpack.server.config.js",
    "buildprod": "rmdir /s /q dist && webpack --mode production --config webpack.server.config.js",
    "start": "node ./dist/server.js",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "mocha unit-tests/**/**test.js --require @babel/register --reporter spec || exit 0'"
  },
  "keywords": [
    "node",
    "javascript",
    "backend",
    "email"
  ],
  "author": "Ankan Sircar",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "prettier": "^1.19.1",
    "querystring": "^0.2.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-loader": "^3.0.3",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "webpack": "^4.41.3",
